<div class="container image-show">
    <div class="image-info">
        <blockquote class="description">
            <p><%= @photo.description %></p>
             <footer><%= @photo.name %></footer>
        </blockquote>
        <a href="<%= edit_photo_path(@photo) %>" class="photo-edit-link">Edit</a>
    </div>

    <%= image_tag @photo.photo.url %>
</div>
